<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paketapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ikonica"
        android:label="Spakuj se"
        android:roundIcon="@drawable/ikonica"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PreporuceniPaketi"></activity>
        <activity android:name=".PodesavanjeActivity" />
        <activity android:name=".SviPaketiActivity" />
        <activity android:name=".PaketActivity" />
        <activity android:name=".PrikazNetPaket" />
        <activity android:name=".PrikazTvPaket" />
        <activity android:name=".PrikaziMobilniPaket" />
        <activity android:name=".ConversationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.WRITE_CALL_LOG" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

</manifest>